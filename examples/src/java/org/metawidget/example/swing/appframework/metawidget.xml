<?xml version="1.0"?>
<metawidget xmlns="http://metawidget.org"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://metawidget.org http://metawidget.org/metawidget-1.0.xsd" version="1.0">

	<swingMetawidget xmlns="java:org.metawidget.swing">
		<inspector>
			<compositeInspector xmlns="java:org.metawidget.inspector.composite" config="CompositeInspectorConfig">
				<inspectors>
					<list>
						<metawidgetAnnotationInspector xmlns="java:org.metawidget.inspector.annotation" config="org.metawidget.inspector.impl.BaseObjectInspectorConfig">
							<actionStyle>
								<class>org.metawidget.inspector.impl.actionstyle.swing.SwingAppFrameworkActionStyle</class>
							</actionStyle>
						</metawidgetAnnotationInspector>
						<propertyTypeInspector xmlns="java:org.metawidget.inspector.propertytype"/>
						<swingAppFrameworkInspector xmlns="java:org.metawidget.inspector.swing"/>			
						<jexlInspector xmlns="java:org.metawidget.inspector.commons.jexl" config="org.metawidget.inspector.impl.BaseObjectInspectorConfig">
							<actionStyle>
								<class>org.metawidget.inspector.impl.actionstyle.swing.SwingAppFrameworkActionStyle</class>
							</actionStyle>
						</jexlInspector>
					</list>
				</inspectors>
			</compositeInspector>
		</inspector>
		<widgetProcessors>
			<list>
				<beansBindingProcessor xmlns="java:org.metawidget.swing.widgetprocessor.binding.beansbinding"/>
			</list>
		</widgetProcessors>
	</swingMetawidget>

</metawidget>