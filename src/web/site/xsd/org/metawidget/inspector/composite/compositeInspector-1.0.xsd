<?xml version="1.0" ?>
<xs:schema targetNamespace="java:org.metawidget.inspector.composite" xmlns="java:org.metawidget.inspector.composite"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" version="1.0">
	
	<xs:element name="compositeInspector">
		<xs:complexType>
			<xs:all>
				<xs:element name="inspectors">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="list">
								<xs:complexType>
									<xs:sequence>
										<xs:any minOccurs="1" maxOccurs="unbounded" processContents="lax"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:all>
			<xs:attribute name="config" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	
</xs:schema>