<?xml version="1.0"?>
<metawidget xmlns="http://metawidget.org"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://metawidget.org http://metawidget.org/xsd/metawidget-1.0.xsd" version="1.0">

	<htmlMetawidgetTag xmlns="java:org.metawidget.jsp.tagext.html">
	
		<inspector>
			<compositeInspector xmlns="java:org.metawidget.inspector.composite"
								xsi:schemaLocation="java:org.metawidget.inspector.composite http://metawidget.org/xsd/org/metawidget/inspector/composite/compositeInspector-1.0.xsd"
								config="CompositeInspectorConfig">
				<inspectors>
					<list>
						<metawidgetAnnotationInspector xmlns="java:org.metawidget.inspector.annotation"/>
						<propertyTypeInspector xmlns="java:org.metawidget.inspector.propertytype"/>
						<jspAnnotationInspector xmlns="java:org.metawidget.inspector.jsp"/>
					</list>
				</inspectors>
			</compositeInspector>
		</inspector>
		
		<widgetBuilder>
			<htmlWidgetBuilder xmlns="java:org.metawidget.jsp.tagext.html.widgetbuilder"/>
		</widgetBuilder>

		<layout>
			<htmlTableLayout xmlns="java:org.metawidget.jsp.tagext.html.layout"/>
		</layout>
		
	</htmlMetawidgetTag>			

</metawidget>