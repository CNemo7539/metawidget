<?xml version="1.0"?>
<metawidget xmlns="http://metawidget.org"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://metawidget.org http://metawidget.org/xsd/metawidget-1.0.xsd" version="1.0">

	<swingMetawidget xmlns="java:org.metawidget.swing">
	
		<inspector>
			<compositeInspector xmlns="java:org.metawidget.inspector.composite" config="CompositeInspectorConfig">
				<inspectors>
					<list>
						<metawidgetAnnotationInspector xmlns="java:org.metawidget.inspector.annotation"/>
						<propertyTypeInspector xmlns="java:org.metawidget.inspector.propertytype"/>
					</list>
				</inspectors>
			</compositeInspector>
		</inspector>
		
		<widgetBuilder>
			<swingWidgetBuilder xmlns="java:org.metawidget.swing.widgetbuilder"/>
		</widgetBuilder>
		
		<widgetProcessors>
			<list>
				<reflectionBindingProcessor xmlns="java:org.metawidget.swing.widgetprocessor.binding.reflection"/>
			</list>
		</widgetProcessors>
		
		<metawidgetLayout>
			<gridBagLayout xmlns="java:org.metawidget.swing.layout"/>
		</metawidgetLayout>
		
	</swingMetawidget>

</metawidget>