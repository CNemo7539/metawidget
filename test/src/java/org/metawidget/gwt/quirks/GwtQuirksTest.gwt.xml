<module>

	<!-- Core GWT -->

	<inherits name="com.google.gwt.user.User"/>
	<inherits name="org.metawidget.GwtMetawidget" />

	<!-- Binding -->
	
  	<generate-with class="org.metawidget.gwt.generator.propertybinding.simple.SimpleBindingProcessorAdapterGenerator">
		<when-type-assignable class="org.metawidget.gwt.quirks.client.model.GwtQuirks"/>
	</generate-with>
	
	<!-- Server -->
	
	<servlet path="/metawidget-inspector" class="org.metawidget.inspector.gwt.remote.server.GwtRemoteInspectorImpl"/>	

</module>
