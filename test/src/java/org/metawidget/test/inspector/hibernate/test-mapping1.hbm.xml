<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.metawidget.test.inspector.hibernate">

    <class name="SuperSuperFoo">
    	<id name="baz"/>    
        <many-to-one name="bar" class="Bar" not-null="true"/>
    </class>

    <subclass name="SuperFoo" extends="SuperSuperFoo"/>

    <subclass name="SubFoo" extends="SuperFoo">
    
    	<property name="abc"/>

		<bag name="def">
			<key />
			<many-to-many class="Baz"/>
		</bag>

		<set name="ghi">
			<key />
			<one-to-many class="org.metawidget.test.inspector.hibernate.Baz"/>
		</set>
		    	
		<list name="jkl">
			<key />
			<list-index />
			<one-to-many class="org.metawidget.test.inspector.hibernate.Baz"/>
		</list>

    </subclass>

</hibernate-mapping>
