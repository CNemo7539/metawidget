<?xml version="1.0"?>
<metawidget xmlns="http://metawidget.org"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://metawidget.org http://metawidget.org/metawidget-1.0.xsd" version="1.0">

	<springMetawidgetTag xmlns="java:org.metawidget.jsp.tagext.html.spring">
		<inspector>
			<compositeInspector xmlns="java:org.metawidget.inspector.composite" config="CompositeInspectorConfig">
				<inspectors>
					<list>
						<propertyTypeInspector xmlns="java:org.metawidget.inspector.propertytype"/>
						<springAnnotationInspector xmlns="java:org.metawidget.inspector.spring"/>
					</list>
				</inspectors>
			</compositeInspector>
		</inspector>
	</springMetawidgetTag>

</metawidget>