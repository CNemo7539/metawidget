<?xml version="1.0"?>
<metawidget xmlns="http://metawidget.org"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://metawidget.org http://metawidget.org/metawidget-1.0.xsd" version="1.0">

	<htmlMetawidget xmlns="java:org.metawidget.faces.component.html">
		<parameter>
			<string>dataTableStyleClass</string>
			<string>aDataTableStyleClass</string>
		</parameter>
		<parameter>
			<string>dataTableColumnClasses</string>
			<string>aDataTableColumnClass1,aDataTableColumnClass2</string>
		</parameter>
		<parameter>
			<string>dataTableRowClasses</string>
			<string>aDataTableRowClass1,aDataTableRowClass2</string>
		</parameter>
		<inspector>		
			<compositeInspector xmlns="java:org.metawidget.inspector.composite" config="CompositeInspectorConfig">
				<inspectors>
					<list>
						<metawidgetAnnotationInspector xmlns="java:org.metawidget.inspector.annotation"/>
						<propertyTypeInspector xmlns="java:org.metawidget.inspector.propertytype"/>
						<java5Inspector xmlns="java:org.metawidget.inspector.java5"/>
						<facesInspector xmlns="java:org.metawidget.inspector.faces"/>
					</list>			
				</inspectors>
			</compositeInspector>
		</inspector>
		<widgetBuilder>
			<compositeWidgetBuilder xmlns="urn:java:org.metawidget.widgetbuilder.composite" config="CompositeWidgetBuilderConfig">
				<widgetBuilders>
					<list>
						<richFacesWidgetBuilder xmlns="java:org.metawidget.faces.component.html.widgetbuilder.richfaces"/>
						<htmlWidgetBuilder xmlns="java:org.metawidget.faces.component.html.widgetbuilder"/>
					</list>
				</widgetBuilders>
			</compositeWidgetBuilder>
		</widgetBuilder>
		<layout>
			<richFacesLayout xmlns="java:org.metawidget.faces.component.html.layout.richfaces"/>
		</layout>
		
	</htmlMetawidget>

</metawidget>