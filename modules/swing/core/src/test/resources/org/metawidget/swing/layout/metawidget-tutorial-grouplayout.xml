<?xml version="1.0"?>
<metawidget xmlns="http://metawidget.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://metawidget.org http://metawidget.org/xsd/metawidget-1.0.xsd" version="1.0">

	<swingMetawidget xmlns="java:org.metawidget.swing">
		<inspector>
			<compositeInspector xmlns="java:org.metawidget.inspector.composite" config="CompositeInspectorConfig">
				<inspectors>
					<array>
						<propertyTypeInspector xmlns="java:org.metawidget.inspector.propertytype" />
						<metawidgetAnnotationInspector xmlns="java:org.metawidget.inspector.annotation" />
						<java5Inspector xmlns="java:org.metawidget.inspector.java5" />
					</array>
				</inspectors>
			</compositeInspector>
		</inspector>
		<metawidgetLayout>
			<tabbedPaneLayoutDecorator xmlns="java:org.metawidget.swing.layout" config="TabbedPaneLayoutDecoratorConfig">
				<layout>
					<groupLayout />
				</layout>
			</tabbedPaneLayoutDecorator>
		</metawidgetLayout>
	</swingMetawidget>

</metawidget>